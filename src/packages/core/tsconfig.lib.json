{
  "extends": "./../../../tsconfig.json",
  "compilerOptions": {
    "outDir": "./../../../out-tsc/lib",
    "target": "es5",
    "declaration": true,
    "inlineSources": true,
    "types": [],
    "lib": [
      "dom",
      "es2018"
    ]
  },
  "angularCompilerOptions": {
    "skipTemplateCodegen": true,
    "strictMetadataEmit": false,
    "enableResourceInlining": true
  },
  "exclude": [
    "./test.ts",
    "**/*.spec.ts",
    "**/*.stories.js",
    "**/*.stories.ts",
    "**/*.stories.mdx"
  ]
}
